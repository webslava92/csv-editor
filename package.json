{
  "name": "csv-editor",
  "version": "0.1.0",
  "private": true,
  "author": "webslava92",
  "description": "csv-editor",
  "homepage": "./",
  "main": "dist/electron.js",
  "build": {
    "appId": "com.example.csv-editor",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "dist/electron.js"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "@date-io/dayjs": "^2.16.0",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.11",
    "@mui/styled-engine-sc": "^5.11.11",
    "@mui/x-date-pickers": "^6.1.0",
    "@tanstack/match-sorter-utils": "^8.8.4",
    "@tanstack/react-table": "^8.8.5",
    "@types/lodash.isequal": "^4.5.6",
    "@types/lodash.omit": "^4.5.7",
    "@types/lodash.throttle": "^4.1.7",
    "@types/qs": "^6.9.7",
    "@types/react": "18.0.28",
    "@types/react-beautiful-dnd": "^13.1.3",
    "@types/react-dom": "18.0.11",
    "body-parser": "^1.20.2",
    "customize-cra": "^1.0.0",
    "dayjs": "^1.11.7",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "lodash.isequal": "^4.5.0",
    "lodash.omit": "^4.5.0",
    "lodash.throttle": "^4.1.1",
    "normalizr": "^3.6.2",
    "object-to-formdata": "^4.4.2",
    "qs": "^6.11.0",
    "react": "18.2.0",
    "react-app-rewired": "^2.2.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "18.2.0",
    "react-final-form": "^6.5.9",
    "react-final-form-arrays": "^3.1.4",
    "react-papaparse": "^4.1.0",
    "react-router-dom": "6",
    "react-scripts": "5.0.1",
    "styled-components": "^5.3.9",
    "typescript": "^4.9.5",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron": "^23.1.3",
    "electron-builder": "^23.6.0",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "lint-staged": ">=13",
    "prettier": "^2.8.1",
    "wait-on": "^7.0.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "tsc": "tsc",
    "electron:dev": "concurrently -k \"tsc -w\" \"cross-env BROWSER=none yarn start\" \"yarn electron:start\"",
    "electron:build": "yarn build && electron-builder -c.extraMetadata.main=dist/electron.js",
    "electron:start": "wait-on tcp:127.0.0.1:3000 && electron .",
    "electron:build-linux": "electron-builder --linux"
  },
  "browserslist": {
    "production": [
      "last 1 electron version",
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 electron version",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "husky": {},
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint . --ext ts --ext tsx --fix"
    ]
  }
}
